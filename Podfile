    pod 'XCGLogger'
    pod 'ReachabilitySwift'
    pod 'SwiftyJSON'
    
    
  target 'SwiftDemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftDemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
